<div class="add__popup hidden">
    <input type="number" id="addIncomeInput">
    <button id="addIncome">Add</button>
</div>
<div class="expense__popup hidden">
    <select name="" id="expenseType">
        <option value="food">Food</option>
        <option value="health">Health</option>
        <option value="bills">Bills</option>
        <option value="entertainment">Entertainment</option>
        <option value="transportation">Transportation</option>
        <option value="miscellaneous">Miscellaneous</option>
    </select>
    <input type="number" id="addExpenseInput">
    <button id="addExpense">Add</button>
</div>